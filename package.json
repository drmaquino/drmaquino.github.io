{
  "dependencies": {
    "bootstrap": "^5.3.1",
    "sweetalert2": "^11.10.6"
  },
  "devDependencies": {
    "@types/node": "^20.11.30"
  },
  "scripts": {
    "test": "node --test",
    "test:one": "node --test --test-name-pattern '/1.1.2.1/'",
    "test:watch": "node --test --watch **/*.test.mjs",
    "test:lcov": "node --test --experimental-test-coverage --test-reporter=spec --test-reporter-destination=stdout --test-reporter=lcov --test-reporter-destination=coverage/lcov.info **/*.test.mjs"
  }
}